{
  "name": "Youtube Click Repulser",
  "description": "Remove clickbait once and for all!",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["helper_functions.js", "webpage.js"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "scripting",
    "storage"
  ]
}
